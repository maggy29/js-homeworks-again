<div class="container">
    <h2 class="title">{{name}}</h2>
    <div class="country-info">
        <p class="country-info__item"><span>Capital:</span> {{capital}}</p>
        <p class="country-info__item"><span>Population:</span> {{population}}
        </p>
        <p class="country-info__item"><span>Languagaes:</span>
        <ul>
            {{#each languages}}
            <li>{{this.name}}</li>
            {{/each}}
        </ul>
        </p>
    </div>
    <img src={{flag}} alt="" width="300">
</div>